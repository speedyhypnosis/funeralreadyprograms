        {
          "_name": "list01",
          "_sourceTheme": "strengthm5",
          "_customHTML": "<section data-bs-version=\"5.1\" class=\"list01 strengthm5\" group=\"List\" plugins=\"mbr-switch-arrow, mobirise-icons2\" mbr-class=\"{\n    'mbr-fullscreen': fullScreen,\n    'mbr-parallax-background': bg.parallax}\">\n    <mbr-parameters>\n        <header>Size</header>\n        <input type=\"checkbox\" title=\"Full Width\" name=\"fullWidth\" checked>\n        <input type=\"range\" inline title=\"Top\" name=\"paddingTop\" min=\"0\" max=\"12\" step=\"1\" value=\"5\">\n        <input type=\"range\" inline title=\"Bottom\" name=\"paddingBottom\" min=\"0\" max=\"12\" step=\"1\" value=\"5\">\n        <input type=\"checkbox\" title=\"Reverse\" name=\"reverseContent\">\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Title\" name=\"showTitle\" checked>\n        <input type=\"checkbox\" title=\"Subtitle\" name=\"showSubtitle\" checked>\n        <input type=\"checkbox\" title=\"Text\" name=\"showText\" checked>\n        <input type=\"color\" title=\"Container BG\" value=\"#f4f6fa\" name=\"containerBg\" selected>\n        <header>List</header>\n        <select title=\"Amount\" name=\"cardAmount\">\n            <option value=\"1\">1</option>\n            <option value=\"2\" selected>2</option>\n            <option value=\"3\">3</option>\n            <option value=\"4\">4</option>\n            <option value=\"5\">5</option>\n            <option value=\"6\">6</option>\n            <option value=\"7\">7</option>\n            <option value=\"8\">8</option>\n        </select>\n        <header>Cards</header>\n        <input type=\"color\" title=\"Icon Color\" value=\"#ffffff\" name=\"iconColor\" selected>\n        <input type=\"color\" title=\"Icon BG\" value=\"#a6adbf\" name=\"iconBg\" selected>\n        <input type=\"color\" title=\"Active Color\" value=\"#ffffff\" name=\"activeColor\" selected>\n        <input type=\"color\" title=\"Active BG\" value=\"#7b86a1\" name=\"activeBg\" selected>\n        <input type=\"color\" title=\"BG\" value=\"#ffffff\" name=\"cardBg\" selected>\n        <header>Background</header>\n        <fieldset type=\"background\" name=\"bg\" parallax>\n            <input type=\"image\" title=\"Image\" value=\"@ADDONS_PATH@/strengthm5/components/_images/background1.jpg\">\n            <input type=\"color\" title=\"Color\" value=\"#ffffff\" selected>\n        </fieldset>\n        <header condition=\"bg.type === 'video'\">Fallback Image</header>\n        <input type=\"image\" title=\"Fallback Image\" value=\"@ADDONS_PATH@/strengthm5/components/_images/background1.jpg\" name=\"fallBackImage\" condition=\"bg.type === 'video'\">\n        <input type=\"checkbox\" title=\"Overlay\" name=\"overlay\" condition=\"bg.type !== 'color'\">\n        <input type=\"color\" title=\"Overlay Color\" name=\"overlayColor\" value=\"#ffffff\" condition=\"overlay && bg.type !== 'color'\">\n        <input type=\"range\" inline title=\"Opacity\" name=\"overlayOpacity\" min=\"0\" max=\"1\" step=\"0.1\" value=\"0.5\" condition=\"overlay && bg.type !== 'color'\">\n    </mbr-parameters>\n\n    <div class=\"mbr-fallback-image disabled\" mbr-if=\"bg.type == 'video'\"></div>\n    <div class=\"mbr-overlay\" mbr-if=\"overlay && bg.type !== 'color'\" opacity=\"{{overlayOpacity}}\" bg-color=\"{{overlayColor}}\"></div>\n\n    <div mbr-class=\"{'container': !fullWidth, 'container-fluid': fullWidth}\">\n        <div class=\"row justify-content-center\">\n            <div class=\"col-12 col-lg-10\">\n                <div class=\"card-wrapper\" mbr-class=\"{'flex-row-reverse' : reverseContent}\">\n                    <div class=\"image-wrapper\">\n                        <a href=\"planning-funeral-5000-budget.html\"><img class=\"little-radius\" src=\"@PROJECT_PATH@/assets/images/funeral-ready-memorial-service-prep-september-2025-a1-2.webp\" alt=\"Funeral Ready Memorial Service Advice\"></a>\n                    </div>\n                    <div class=\"content-wrapper\">\n                        <div class=\"content-wrap\" mbr-if=\"showTitle || showSubtitle || showText\">\n                            <div class=\"title-wrapper\" mbr-if=\"showTitle || showSubtitle\">\n                                <h2 class=\"mbr-section-title mbr-fonts-style\" data-app-selector=\".mbr-section-title\" mbr-theme-style=\"display-2\" mbr-if=\"showTitle\"><b>Funeral Planning</b></h2>\n                                <h3 class=\"mbr-section-subtitle mbr-fonts-style\" data-app-selector=\".mbr-section-subtitle\" mbr-theme-style=\"display-2\" mbr-if=\"showSubtitle\"><b>Guidance</b></h3>\n                            </div>\n                            <div class=\"text-wrapper\">\n                                <p class=\"mbr-text mbr-fonts-style\" data-app-selector=\".mbr-text, .text-wrapper\" mbr-theme-style=\"display-7\" mbr-if=\"showText\">\n                                    Get expert advice for the decisions families face during difficult times. Our guides help you navigate funeral planning with confidence and avoid common mistakes.\n                                </p>\n                            </div>\n                        </div>\n                        <div id=\"bootstrap-accordion\" class=\"panel-group accordionStyles accordion\" role=\"tablist\" aria-multiselectable=\"true\">\n                            <div class=\"card little-radius\">\n                                <div class=\"card-header\" role=\"tab\" id=\"headingOne\">\n                                    <a role=\"button\" class=\"panel-title collapsed\" data-toggle=\"collapse\" data-bs-toggle=\"collapse\" data-core href=\"#collapse1\" aria-expanded=\"false\" aria-controls=\"collapse1\">\n                                        <h4 class=\"panel-title-edit mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".panel-title-edit\"><b>How to Plan a Dignified Funeral on a $5,000 Budget</b></h4>\n                                        <div class=\"icon-wrapper\">\n                                            <span class=\"sign mbr-iconfont mobi-mbri-arrow-down mobi-mbri\"></span>\n                                        </div>\n                                    </a>\n                                </div>\n                                <div id=\"collapse1\" class=\"panel-collapse noScroll collapse\" role=\"tabpanel\" aria-labelledby=\"headingOne\" data-parent=\"#accordion\" data-bs-parent=\"#accordion\">\n                                    <div class=\"panel-body\">\n                                        <p class=\"panel-text mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".panel-text\">\n                                            Linda Johnson was sitting in her car outside Elmwood Cemetery in Memphis, crying and doing math on the back of an envelope. <a href=\"planning-funeral-5000-budget.html\" class=\"text-primary\">Read more &gt;</a></p>\n                                    </div>\n                                </div>\n                            </div>\n                            <div class=\"card little-radius\" mbr-if=\"cardAmount > 1\">\n                                <div class=\"card-header\" role=\"tab\" id=\"headingTwo\">\n                                    <a role=\"button\" class=\"panel-title collapsed\" data-toggle=\"collapse\" data-bs-toggle=\"collapse\" data-core href=\"#collapse2\" aria-expanded=\"false\" aria-controls=\"collapse2\">\n                                        <h4 class=\"panel-title-edit mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".panel-title-edit\"><b>Sarah's 12-Hour Funeral Program Nightmare&nbsp;</b></h4>\n                                        <div class=\"icon-wrapper\">\n                                            <span class=\"sign mbr-iconfont mobi-mbri-arrow-down mobi-mbri\"></span>\n                                        </div>\n                                    </a>\n                                </div>\n                                <div id=\"collapse2\" class=\"panel-collapse noScroll collapse\" role=\"tabpanel\" aria-labelledby=\"headingTwo\" data-parent=\"#accordion\" data-bs-parent=\"#accordion\">\n                                    <div class=\"panel-body\">\n                                        <p class=\"panel-text mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".panel-text\">\n                                            At 11:30 PM on a Tuesday, Sarah Chen sat at her dining room table in Minneapolis, surrounded by paper scraps, glue sticks, and three different printers that kept jamming. <a href=\"sarah-funeral-programs-diy-disaster-12-hours.html\" class=\"text-primary\">Read more &gt;</a></p>\n                                    </div>\n                                </div>\n                            </div>\n                            <div class=\"card little-radius\" mbr-if=\"cardAmount > 2\">\n                                <div class=\"card-header\" role=\"tab\" id=\"headingThree\">\n                                    <a role=\"button\" class=\"panel-title collapsed\" data-toggle=\"collapse\" data-bs-toggle=\"collapse\" data-core href=\"#collapse3\" aria-expanded=\"false\" aria-controls=\"collapse3\">\n                                        <h4 class=\"panel-title-edit mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".panel-title-edit\">\n                                            <b>How often should I train with a fitness trainer?</b>\n                                        </h4>\n                                        <div class=\"icon-wrapper\">\n                                            <span class=\"sign mbr-iconfont mobi-mbri-arrow-down mobi-mbri\"></span>\n                                        </div>\n                                    </a>\n                                </div>\n                                <div id=\"collapse3\" class=\"panel-collapse noScroll collapse\" role=\"tabpanel\" aria-labelledby=\"headingThree\" data-parent=\"#accordion\" data-bs-parent=\"#accordion\">\n                                    <div class=\"panel-body\">\n                                        <p class=\"panel-text mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".panel-text\">\n                                            It depends on your goals and schedule. Many clients train 2–3 times per\n                                            week,\n                                            but\n                                            even once a week with proper guidance can deliver results.\n                                        </p>\n                                    </div>\n                                </div>\n                            </div>\n                            <div class=\"card little-radius\" mbr-if=\"cardAmount > 3\">\n                                <div class=\"card-header\" role=\"tab\" id=\"headingFour\">\n                                    <a role=\"button\" class=\"panel-title collapsed\" data-toggle=\"collapse\" data-bs-toggle=\"collapse\" data-core href=\"#collapse4\" aria-expanded=\"false\" aria-controls=\"collapse4\">\n                                        <h4 class=\"panel-title-edit mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".panel-title-edit\">\n                                            <b>Will I also get nutrition advice?</b>\n                                        </h4>\n                                        <div class=\"icon-wrapper\">\n                                            <span class=\"sign mbr-iconfont mobi-mbri-arrow-down mobi-mbri\"></span>\n                                        </div>\n                                    </a>\n                                </div>\n                                <div id=\"collapse4\" class=\"panel-collapse noScroll collapse\" role=\"tabpanel\" aria-labelledby=\"headingFour\" data-parent=\"#accordion\" data-bs-parent=\"#accordion\">\n                                    <div class=\"panel-body\">\n                                        <p class=\"panel-text mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".panel-text\">\n                                            Yes. Many trainers provide basic nutrition guidance or meal planning tips to\n                                            complement workouts and accelerate results.\n                                        </p>\n                                    </div>\n                                </div>\n                            </div>\n                            <div class=\"card little-radius\" mbr-if=\"cardAmount > 4\">\n                                <div class=\"card-header\" role=\"tab\" id=\"headingFive\">\n                                    <a role=\"button\" class=\"panel-title collapsed\" data-toggle=\"collapse\" data-bs-toggle=\"collapse\" data-core href=\"#collapse5\" aria-expanded=\"false\" aria-controls=\"collapse5\">\n                                        <h4 class=\"panel-title-edit mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".panel-title-edit\">\n                                            <b>How long does it take to see results?</b>\n                                        </h4>\n                                        <div class=\"icon-wrapper\">\n                                            <span class=\"sign mbr-iconfont mobi-mbri-arrow-down mobi-mbri\"></span>\n                                        </div>\n                                    </a>\n                                </div>\n                                <div id=\"collapse5\" class=\"panel-collapse noScroll collapse\" role=\"tabpanel\" aria-labelledby=\"headingFive\" data-parent=\"#accordion\" data-bs-parent=\"#accordion\">\n                                    <div class=\"panel-body\">\n                                        <p class=\"panel-text mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".panel-text\">\n                                            Most clients notice improvements in strength, energy, and mood within 4–6\n                                            weeks,\n                                            though long-term changes depend on consistency and effort.\n                                        </p>\n                                    </div>\n                                </div>\n                            </div>\n                            <div class=\"card little-radius\" mbr-if=\"cardAmount > 5\">\n                                <div class=\"card-header\" role=\"tab\" id=\"headingSix\">\n                                    <a role=\"button\" class=\"panel-title collapsed\" data-toggle=\"collapse\" data-bs-toggle=\"collapse\" data-core href=\"#collapse6\" aria-expanded=\"false\" aria-controls=\"collapse6\">\n                                        <h4 class=\"panel-title-edit mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".panel-title-edit\">\n                                            <b>Can a fitness trainer help with weight loss?</b>\n                                        </h4>\n                                        <div class=\"icon-wrapper\">\n                                            <span class=\"sign mbr-iconfont mobi-mbri-arrow-down mobi-mbri\"></span>\n                                        </div>\n                                    </a>\n                                </div>\n                                <div id=\"collapse6\" class=\"panel-collapse noScroll collapse\" role=\"tabpanel\" aria-labelledby=\"headingSix\" data-parent=\"#accordion\" data-bs-parent=\"#accordion\">\n                                    <div class=\"panel-body\">\n                                        <p class=\"panel-text mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".panel-text\">\n                                            Absolutely. Trainers combine cardio, strength training, and healthy\n                                            lifestyle\n                                            strategies to help clients lose weight in a safe and sustainable way.\n                                        </p>\n                                    </div>\n                                </div>\n                            </div>\n                            <div class=\"card little-radius\" mbr-if=\"cardAmount > 6\">\n                                <div class=\"card-header\" role=\"tab\" id=\"headingSeven\">\n                                    <a role=\"button\" class=\"panel-title collapsed\" data-toggle=\"collapse\" data-bs-toggle=\"collapse\" data-core href=\"#collapse7\" aria-expanded=\"false\" aria-controls=\"collapse7\">\n                                        <h4 class=\"panel-title-edit mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".panel-title-edit\">\n                                            <b>What if I have an injury or health condition?</b>\n                                        </h4>\n                                        <div class=\"icon-wrapper\">\n                                            <span class=\"sign mbr-iconfont mobi-mbri-arrow-down mobi-mbri\"></span>\n                                        </div>\n                                    </a>\n                                </div>\n                                <div id=\"collapse7\" class=\"panel-collapse noScroll collapse\" role=\"tabpanel\" aria-labelledby=\"headingSeven\" data-parent=\"#accordion\" data-bs-parent=\"#accordion\">\n                                    <div class=\"panel-body\">\n                                        <p class=\"panel-text mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".panel-text\">\n                                            Fitness trainers adapt programs to your needs, focusing on safe exercises\n                                            that\n                                            support recovery, mobility, and overall health.\n                                        </p>\n                                    </div>\n                                </div>\n                            </div>\n                            <div class=\"card little-radius\" mbr-if=\"cardAmount > 7\">\n                                <div class=\"card-header\" role=\"tab\" id=\"headingEight\">\n                                    <a role=\"button\" class=\"panel-title collapsed\" data-toggle=\"collapse\" data-bs-toggle=\"collapse\" data-core href=\"#collapse8\" aria-expanded=\"false\" aria-controls=\"collapse8\">\n                                        <h4 class=\"panel-title-edit mbr-fonts-style\" mbr-theme-style=\"display-4\" data-app-selector=\".panel-title-edit\">\n                                            <b>Why hire a fitness trainer instead of working out alone?</b>\n                                        </h4>\n                                        <div class=\"icon-wrapper\">\n                                            <span class=\"sign mbr-iconfont mobi-mbri-arrow-down mobi-mbri\"></span>\n                                        </div>\n                                    </a>\n                                </div>\n                                <div id=\"collapse8\" class=\"panel-collapse noScroll collapse\" role=\"tabpanel\" aria-labelledby=\"headingEight\" data-parent=\"#accordion\" data-bs-parent=\"#accordion\">\n                                    <div class=\"panel-body\">\n                                        <p class=\"panel-text mbr-fonts-style\" mbr-theme-style=\"display-7\" data-app-selector=\".panel-text\">\n                                            A trainer ensures you stay motivated, accountable, and safe while maximizing\n                                            results. They provide structure, feedback, and expertise that self-training\n                                            often\n                                            lacks.\n                                        </p>\n                                    </div>\n                                </div>\n                            </div>\n                        </div>\n                    </div>\n                </div>\n            </div>\n        </div>\n    </div>\n</section>",
